    "@types/express": "^4.17.17",
    "@types/flat": "^5.0.2",
    "@types/jest": "29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.5.7",
    "@types/nodemailer": "^6.4.9",
    "@types/otplib": "^10.0.0",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "jest": "29.6.4",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^libs/(.*)$": "<rootDir>/libs/$1",
      "@can/aws/(.*)": "<rootDir>/libs/aws/src/$1",
      "@can/aws": "<rootDir>/libs/aws/src",
      "@can/state-machine/(.*)": "<rootDir>/libs/state-machine/src/$1",
      "@can/state-machine": "<rootDir>/libs/state-machine/src",
      "@can/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@can/common": "<rootDir>/libs/common/src",
      "@can/notification/(.*)": "<rootDir>/libs/notification/src/$1",
      "@can/notification": "<rootDir>/libs/notification/src"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**.{ts}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}